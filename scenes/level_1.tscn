[sub_resource type="GDScript" id="GDScript_8x38w"]
resource_name = "tabmanager"
script/source = "extends VBoxContainer

@onready var panels_container = $panels

var panels: Array

func _ready():
	panels = panels_container.get_children()
	show_tab(0)

func show_tab(idx: int):
	for panel in panels:
		panel.hide()
	
	panels[idx].show()

func _on_tab_1_pressed() -> void:
	show_tab(0)


func _on_tab_2_pressed() -> void:
	show_tab(1)


func _on_color_pressed() -> void:
	show_tab(2)

func _on_eyes_pressed() -> void:
	show_tab(3)


func _on_mouth_pressed() -> void:
	show_tab(4)
"

